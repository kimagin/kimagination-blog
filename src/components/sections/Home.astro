---
import SectionLayout from '../../layouts/SectionLayout.astro'

type Props = {}

const {} = Astro.props

const sectionID = 'home'
---

<SectionLayout
  id={sectionID}
  title="Home"
  fullScreen
  centered
  classes="flex-col">
  <h1>Welcome to the best Astro Template</h1>
  <div class="flex gap-2">
    <button class="btn btn-primary rounded-lg">Get Started</button>
    <button class="btn btn-primary rounded-lg btn-outline">learn more</button>
  </div>
</SectionLayout>
